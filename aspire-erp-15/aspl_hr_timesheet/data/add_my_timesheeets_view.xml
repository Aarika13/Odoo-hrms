<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ir_cron_add_my_timesheets" model="ir.cron">
            <field name="name">Add Next Month My Timesheet</field>
            <field name="model_id" ref="model_hr_timesheet_sheet"/>
            <field name="state">code</field>
            <field name="code">model.add_next_month_my_timesheet()</field>
            <field name="priority">1</field>
            <field name="interval_number">1</field>
