<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_attendance_wizard" model="ir.ui.view">
            <field name="name">attendance wizard</field>
            <field name="model">attendance.wizard</field>
            <field name="arch" type="xml">
                <form string="Attendance">
                    <group>
                        <group>
                            <field name="start_date"/>
                            <field name="end_date"/>
                        </group>
                        <group>
                        </group>
                    </group>
                    <footer>
                        <button name="generate_attendance" string="Generate" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
